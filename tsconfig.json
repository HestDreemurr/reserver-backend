{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": ["./src/*"],
      "#sequelize": ["./sequelize"]
    }
  },
  "include": [
    "src/**/*.ts"
  ]
}
